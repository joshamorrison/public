# LangChain Configuration
LANGCHAIN_API_KEY=your_langsmith_key
LANGCHAIN_TRACING_V2=true
LANGCHAIN_PROJECT=multi-agent-orchestration
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key
OPENAI_MODEL=gpt-4-turbo-preview
OPENAI_TEMPERATURE=0.1
OPENAI_MAX_TOKENS=4000

# Anthropic Configuration
ANTHROPIC_API_KEY=your_anthropic_key
ANTHROPIC_MODEL=claude-3-sonnet-20240229

# AWS Bedrock Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_DEFAULT_REGION=us-east-1
BEDROCK_MODEL_ID=anthropic.claude-3-sonnet-20240229-v1:0
BEDROCK_RUNTIME_REGION=us-east-1

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/agents_db
REDIS_URL=redis://localhost:6379/0

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
API_WORKERS=4
API_SECRET_KEY=your-secret-key-here

# Agent Configuration
MAX_CONCURRENT_AGENTS=10
AGENT_TIMEOUT_SECONDS=300
MAX_RETRIES=3
BACKOFF_FACTOR=2.0

# Tool Configuration
WEB_SEARCH_API_KEY=your_serpapi_key
GOOGLE_SEARCH_ENGINE_ID=your_google_cse_id
SELENIUM_GRID_URL=http://localhost:4444/wd/hub

# Monitoring Configuration
PROMETHEUS_PORT=8001
LOG_LEVEL=INFO
STRUCTURED_LOGGING=true

# Celery Configuration (for task queue)
CELERY_BROKER_URL=redis://localhost:6379/1
CELERY_RESULT_BACKEND=redis://localhost:6379/2
CELERY_WORKER_CONCURRENCY=4

# Security Configuration
JWT_SECRET_KEY=your-jwt-secret-key
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=60

# Rate Limiting
RATE_LIMIT_PER_MINUTE=100
RATE_LIMIT_PER_HOUR=1000

# Performance Configuration
MAX_WORKFLOW_DEPTH=10
MAX_AGENT_MEMORY_SIZE=1000
AGENT_CLEANUP_INTERVAL_HOURS=24

# External Services
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/your/slack/webhook
EMAIL_SMTP_SERVER=smtp.gmail.com
EMAIL_SMTP_PORT=587
EMAIL_FROM=agents@yourcompany.com
EMAIL_PASSWORD=your_email_password

# Storage Configuration
S3_BUCKET=your-agent-storage-bucket
LOCAL_STORAGE_PATH=./storage/

# Environment
ENVIRONMENT=development
DEBUG=True